{
  "/api/1": {
    "headers": {
      "Content-type": "application/json",
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Headers": "X-Requested-With"
    },
    "statusCode": 200,
    "body": {
      "name": "Something",
      "data": [1, 2, 3, 4, 5, 6]
    }
  },
  "/api/2": {
    "headers": {
      "Content-type": "application/json",
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Headers": "X-Requested-With",
      "X-Application-Token": "b4c0n"
    },
    "statusCode": 201,
    "body": {
      "message": "Created",
      "data": {"user": "anonymous"}
    }
  },
  "/api/error": {
    "headers": {
      "Content-Type": "text/html"
    },
    "statusCode": 500
  },
  "/api/user/\\d{1,3}/profile": {
    "headers": {
      "Content-type": "application/json",
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Headers": "X-Requested-With"
    },
    "statusCode": 200,
    "body": {
      "name": "Looser User",
      "data": {
        "id": 12345,
        "score": 500
      }
    }
  }
}
